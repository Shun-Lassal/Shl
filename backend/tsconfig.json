{
  "compilerOptions": {
    "target": "ES2020",
    "module": "node20",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "types": [
      "node",
      "express",
      "cookie-parser",
      "cors",
      "swagger-ui-express",
      "./src/types/swagger-jsdoc"
    ]
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "prisma", "prisma.config.ts"]
}
